{
  "description": "Slim starter + service",
  "name": "autokz/sevice_name",
  "minimum-stability": "stable",
  "license": "proprietary",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "doctrine/migrations": "^3.1",
    "doctrine/orm": "*",
    "laminas/laminas-config-aggregator": "^1.5",
    "league/oauth2-server": "^8.2",
    "monolog/monolog": "^2.2",
    "php-di/php-di": "^6.3",
    "ramsey/uuid": "^4.1",
    "slim/psr7": "^1.3",
    "slim/slim": "^4.7",
    "symfony/console": "^5.2",
    "symfony/validator": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Ilyar",
      "email": "ilyar_yusupov52@gmail.com",
      "homepage": "https://github.com/Red-373"
    }
  ],
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t api/public api/public/index.php"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^1.5",
    "overtrue/phplint": "^2.3",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.5",
    "vimeo/psalm": "^4.7"
  }
}
